<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
  <PackageId>MyBatis.NET</PackageId>
  <Version>1.2.0-alpha</Version>
  <Authors>Hammond</Authors>
  <Description>Lightweight MyBatis port for .NET. XML mappers, runtime proxy, and transaction support.</Description>
  <PackageLicenseExpression>MIT</PackageLicenseExpression>
  <PackageProjectUrl>https://github.com/hammonddev/MyBatis.NET</PackageProjectUrl>
  <RepositoryUrl>https://github.com/hammonddev/MyBatis.NET</RepositoryUrl>
  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
</PropertyGroup>


  <ItemGroup>
   <PackageReference Include="Microsoft.Data.SqlClient" Version="7.0.0-preview2.25289.6" /> 
  </ItemGroup>
</Project>
